@model List<Coche>

<table class="table table-borderless">
    <thead>
        <tr>
            <th>Marca</th>
            <th>Modelo</th>
            <th>Imagen</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Coche item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Marca)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Modelo)
                </td>
                <td>
                    <img src="@item.Imagen" style="width:150px;height:100px;" />
                </td>
            </tr>
        }
    </tbody>
</table>
